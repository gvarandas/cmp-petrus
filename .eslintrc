{
  "extends": "airbnb",
  "rules": {
    "max-len": [
      "error",
      {
        "code": 200
      }
    ],
    "class-methods-use-this": 0,
    "react/prefer-stateless-function": 0,
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [
          "Link"
        ],
        "specialLink": [
          "to"
        ]
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "consistent": true
      }
    ]
  }
}